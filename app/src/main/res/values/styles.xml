<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">#fff</item>
        <item name="boxStrokeWidth">2dp</item>
    </style>
    <style name="editTextStyle" parent="Base.Widget.MaterialComponents.TextInputLayout">
        <item name="materialThemeOverlay">
            @style/ThemeOverlay.MaterialComponents.TextInputEditText.OutlinedBox
        </item>
        <item name="boxCollapsedPaddingTop">0dp</item>
        <item name="boxStrokeColor">@color/blueColor</item>

    </style>
    <style name="customfontstyle" parent="@android:style/TextAppearance.Small">
        <item name="android:fontFamily">@font/inter_medium</item>
    </style>

    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/drop_arrow</item>
        <item name="android:drawablePadding">12dp</item>
    </style>
    <style name="MenuButtonsStyle"> <item name="fab_size">normal</item>
        <item name="fab_shadowColor">#444</item>
        <item name="elevation">4dp</item>
        <item name="borderWidth">1dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="fab_colorNormal">@android:color/white</item>
        <item name="fab_colorPressed">@color/blueColor</item> </style>
    <style name="yourTheme" parent="Theme.AppCompat">
        ...
        <item name="fab_size">normal</item>
        <item name="fab_shadowColor">#66000000</item>
        <item name="backgroundTint">@color/blueColor</item>
        <item name="fab_colorNormal">@color/blueColor</item>
        <item name="fab_colorPressed">@color/blueColor</item>
        <item name="menu_colorNormal">@color/blueColor</item>
        <item name="menu_colorPressed">@color/blueColor</item>
